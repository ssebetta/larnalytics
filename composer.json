{
    "name": "ssebetta/larnalytics",
    "description": "A simple web analytics tool for laravel projects",
    "type": "package",
    "version": "1.0.0",
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "jenssegers/agent": "^2.6",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ssebetta\\Larnalytics\\": "src/"
        }
    },
    "extra": {
	"laravel": {
            "providers": [
                "SiteVisitAnalytics\\AnalyticsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Harold Peter Ssebetta",
            "email": "haroldpeter6@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
